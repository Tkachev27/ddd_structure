{
  "name": "cfg-web-gui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "dev": "ng serve --port 4201 --host 0.0.0.0 --disable-host-check -c debug",
    "build": "ng build",
    "test": "ng test",
    "test:ci": "ng test --no-watch --no-progress --browsers=ChromeHeadlessCI",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "bundle-report": "npm run build -- --configuration production --output-path=dist --stats-json && webpack-bundle-analyzer dist/stats.json",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "ng run cfg-web-gui:storybook --host 0.0.0.0",
    "build-storybook": "ng run cfg-web-gui:build-storybook"
